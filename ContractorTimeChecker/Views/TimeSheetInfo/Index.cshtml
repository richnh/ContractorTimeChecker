@model IEnumerable<ContractorTimeChecker.Models.TimesheetEntryInfo>

@{
    ViewBag.Title = "TimeSheet Information";
}

<div class="row">
    <h2>View Timesheet Information</h2>
    <div class="panel panel-default">
        <div class="panel-body">View all contractor information</div>
    </div>
</div>

    <table class="table table-bordered table-striped">
    <tr>
        <th>
            Candidate Name
        </th>
        <th>
            Client Name
        </th>
        <th>
            Job Title
        </th>
        <th>
            Date
        </th>
        <th>
            Duration
        </th>
        <th>
            Placement Type
        </th>
    </tr>
        @foreach (var item in @Model)
        {
            <tr>
                <td>
                    @Html.Encode(item.CandidateName)
                </td>

                <td>
                    @Html.Encode(item.ClientName)
                </td>

                <td>
                    @Html.Encode(item.JobTitle)
                </td>

                <td>
                    @Html.Encode(item.Date)
                </td>

                <td>
                    @Html.Encode(item.Duration)
                </td>

                <td>
                    @if(item.PlacementType != null) { @Html.Encode((ContractorTimeChecker.Models.Enums.PlacementTypeEnum)item.PlacementType) }            
                </td>
            </tr>
        }        
    </table> 
